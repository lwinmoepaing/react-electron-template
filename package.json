{
  "name": "LMP",
  "version": "0.0.2",
  "description": "This app is frame for react-electorn.",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "dev": "npm run watch",
    "watch": "webpack --config webpack.common.js --watch",
    "build": "webpack --config webpack.prod.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "make:linux": "electron-builder --linux",
    "make:macos": "electron-builder --mac",
    "make:win": "electron-builder --win --x64",
    "make:win-single": "electron-builder --win portable",
    "deploy": "electron-builder --win --x64 --p always"
  },
  "keywords": [],
  "author": "Lwin Moe Paing",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:lwinmoepaing/react-electron-template.git"
  },
  "bugs": {
    "url": "https://github.com/lwinmoepaing/react-electron-template/issues"
  },
  "homepage": "https://github.com/lwinmoepaing/react-electron-template#readme",
  "build": {
    "productName": "Lmp Frame",
    "appId": "com.lmp.frame",
    "files": [
      "build/**/*",
      "config/**/*",
      "public/**/*",
      "server/**/*",
      "src/**/*",
      "utils/**/*",
      "index.js"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "public/assets/images/app_logo.png",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 430,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "icon": "public/assets/images/app_logo.png",
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "category": "Development"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "lwinmoepaing",
        "repo": "react-electron-template"
      }
    ]
  },
  "devDependencies": {
    "@swc/core": "^1.2.106",
    "electron": "^15.0.0",
    "electron-builder": "^22.13.1",
    "electron-reload": "^2.0.0-alpha.1",
    "esbuild": "^0.8.57",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@hapi/joi": "^17.1.1",
    "@mui/icons-material": "^5.0.5",
    "@mui/lab": "^5.0.0-alpha.53",
    "@mui/material": "^5.0.6",
    "axios": "^0.24.0",
    "babel-loader": "^8.2.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "bookshelf": "^1.2.0",
    "cors": "^2.8.5",
    "css-loader": "^6.3.0",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "electron-updater": "^4.3.9",
    "express": "^4.17.1",
    "express-list-endpoints": "^6.0.0",
    "formik": "^2.2.9",
    "helmet": "^4.6.0",
    "i18next": "^21.2.6",
    "ip-regex": "^4.3.0",
    "joi-objectid": "^4.0.2",
    "knex": "^0.95.11",
    "localforage": "^1.10.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.12.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "sqlite3": "^5.0.2",
    "style-loader": "^3.3.0",
    "uuidv4": "^6.2.12",
    "webpack": "^5.54.0",
    "webpack-loader": "^0.0.1",
    "webpack-merge": "^5.8.0",
    "yup": "^0.32.11"
  }
}
